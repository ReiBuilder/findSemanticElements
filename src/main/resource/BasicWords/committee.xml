<entry_list version="1.0">
	<entry id="committee"><ew>committee</ew><subj>GV-2a</subj><hw>com*mit*tee</hw><sound><wav>commit05.wav</wav><wpr>ku-!mi-tE</wpr></sound><pr>kə-ˈmi-tē, <it>sense 1 also</it> ˌkä-mi-ˈtē</pr><fl>noun</fl><def><date>15th century</date> <sn>1</sn> <ssl>archaic</ssl> <dt>:a person to whom a charge or trust is <fw>committed</fw></dt> <sn>2 a</sn> <dt>:a body of persons delegated to consider, investigate, take action on, or report on some matter</dt> <sd>especially</sd> <dt>:a group of fellow legislators chosen by a legislative body to give consideration to legislative matters</dt>  <sn>b</sn> <dt>:a self-constituted organization for the promotion of a <fw>common</fw> object</dt></def></entry>
	<entry id="committee of the whole"><ew>committee of the whole</ew><grp>committee of</grp><grp>committee the</grp><grp>committee whole</grp><grp>the whole</grp><grp>of the</grp><grp>of whole</grp><grp>the whole</grp><subj>GV</subj><hw>committee of the whole</hw><def><date>1775</date><dt>:the whole membership of a legislative house sitting as a committee and operating under informal rules</dt></def></entry>
	<entry id="political action committee"><ew>political action committee</ew><grp>political action</grp><grp>political committee</grp><grp>action committee</grp><subj>GV</subj><hw>political action committee</hw><fl>noun</fl><def><date>1944</date><dt>:a group formed (as by an industry or an issue-oriented organization) to raise and contribute money to the campaigns of candidates likely to advance the group's interests</dt></def></entry>
	<entry id="school committee"><ew>school committee</ew><subj>ED#GV</subj><hw>school committee</hw><fl>noun</fl><def><date>1787</date><dt>:<sx>school board</sx></dt></def></entry>
	<entry id="standing committee"><ew>standing committee</ew><subj>GV</subj><hw>standing committee</hw><fl>noun</fl><def><date>circa 1636</date><dt>:a permanent committee especially of a legislative body</dt></def></entry>
	<entry id="steering committee"><ew>steering committee</ew><subj>GV</subj><hw>steering committee</hw><fl>noun</fl><def><date>1887</date><dt>:a managing or directing committee</dt> <sd>specifically</sd> <dt>:a committee that determines the order in which business will be taken up in a United States legislative body</dt></def></entry>
	<entry id="vigilance committee"><ew>vigilance committee</ew><subj>LW#SY</subj><hw>vigilance committee</hw><fl>noun</fl><def><date>1835</date><dt>:a committee of vigilantes</dt></def></entry>
