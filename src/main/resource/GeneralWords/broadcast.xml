	<entry id="broadcast[1]"><ew>broadcast</ew><subj>BR-2#BR-3</subj><hw hindex="1">broad*cast</hw><sound><wav>broadc01.wav</wav><wpr>!brod-+kast</wpr></sound><pr>ˈbrȯd-ˌkast</pr><fl>adjective</fl><def><date>1767</date> <sn>1</sn> <dt>:cast or scattered in all directions</dt> <sn>2</sn> <dt>:made public by means of radio or television</dt> <sn>3</sn> <dt>:of or relating to radio or television <fw>broadcasting</fw></dt></def></entry>
