<entry_list version="1.0">
	<entry id="leather[1]"><ew>leather</ew><subj>AN-2#ZM-2#FR-1#FR-3#CL-3</subj><hw hindex="1">leath*er</hw><sound><wav>leathe01.wav</wav><wpr>!le-Dur</wpr></sound><pr>ˈle-t͟hər</pr><fl>noun</fl><et>Middle English <it>lether,</it> from Old English <it>lether-;</it> akin to Old High German <it>leder</it> leather, Old Irish <it>lethar</it></et><def><date>13th century</date> <sn>1</sn> <dt>:animal skin dressed for use</dt> <sn>2</sn> <dt>:the flap of the ear of a dog <dx>see <dxt>dog <dxn>illustration</dxn></dxt></dx></dt> <sn>3</sn> <dt>:something wholly or partly made of leather</dt></def><uro><ure>leather</ure> <fl>adjective</fl></uro><uro><ure>leath*er*like</ure><sound><wav>leathe02.wav</wav><wpr>!le-Dur-+lIk</wpr></sound> <pr>-ˌlīk</pr> <fl>adjective</fl></uro></entry>
	<entry id="leather[2]"><ew>leather</ew><subj>FR-1</subj><hw hindex="2">leather</hw><fl>verb</fl><in><if>leath*ered</if></in><in><if>leath*er*ing</if><sound><wav>leathe03.wav</wav><wpr>!le-Du-riN</wpr></sound> <pr>ˈle-t͟hə-riŋ, ˈlet͟h-riŋ</pr></in><def><vt>transitive verb</vt><date>13th century</date> <sn>1</sn> <dt>:to cover with leather</dt> <sn>2</sn> <dt>:to beat with a strap :<sx>thrash</sx></dt></def></entry>
	<entry id="leather-lunged"><ew>leather-lunged</ew><hw>leath*er–lunged</hw><sound><wav>leath01v.wav</wav><wpr>!le-Dur-+luNd</wpr></sound><pr>ˈle-t͟hər-ˌləŋd</pr><fl>adjective</fl><def><date>1846</date><dt>:having an inordinately loud voice <vi><it>leather–lunged</it> singers</vi></dt></def></entry>
	<entry id="buff leather"><ew>buff leather</ew><subj>FR</subj><hw>buff leather</hw><fl>noun</fl><def><date>1580</date><dt>:a strong supple oil-tanned leather produced chiefly from cattle hides</dt></def></entry>
	<entry id="glove leather"><ew>glove leather</ew><subj>FR</subj><hw>glove leather</hw><fl>noun</fl><def><date>1721</date><dt>:a soft lightweight leather</dt></def></entry>
	<entry id="hell-for-leather[1]"><ew>hell-for-leather</ew><hw hindex="1">hell–for–leather</hw><fl>adverb</fl><def><date>1889</date><dt>:in a hell-for-leather manner :at full speed <vi>rode <it>hell–for–leather</it> down the trail</vi></dt></def></entry>
	<entry id="hell-for-leather[2]"><ew>hell-for-leather</ew><hw hindex="2">hell–for–leather</hw><fl>adjective</fl><def><date>1920</date><dt>:marked by determined recklessness, great speed, or lack of restraint <vi>a cocky, <it>hell–for–leather</it> fighting man <aq>H. H. Martin</aq></vi></dt></def></entry>
	<entry id="kid leather"><ew>kid leather</ew><subj>CL-2#FR-1</subj><hw>kid leather</hw><fl>noun</fl><def><date>1687</date> <sn>1</sn> <dt>:a soft pliable leather made from kidskin</dt> <sn>2</sn> <dt>:a glove leather made from lambskin or goatskin</dt></def></entry>
	<entry id="napa leather"><ew>napa leather</ew><subj>FR</subj><hw>napa leather</hw><fl>noun</fl><et><it>Napa,</it> California</et><def><date>1897</date><dt>:a glove leather made by tawing sheepskins with a soap-and-oil mixture</dt> <sd>also</sd> <dt>:a similar soft leather</dt></def></entry>
	<entry id="patent leather"><ew>patent leather</ew><subj>FR</subj><hw>pat*ent leather</hw><sound><wav>patent08.wav</wav><wpr>!pa-t/un(t)-!le-Dur</wpr></sound><pr>ˈpa-tᵊn(t)-, <it>British usually</it> ˈpā-</pr><fl>noun</fl><def><date>1829</date><dt>:a leather with a hard smooth glossy surface</dt></def></entry>
