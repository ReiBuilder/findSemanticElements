	<entry id="holy day"><ew>holy day</ew><subj>RE#CA#RJ#RC</subj><hw>holy day</hw><fl>noun</fl><def><date>before 12th century</date><dt>:a day set aside for special religious observance</dt></def></entry>
	<entry id="holy day of obligation"><ew>holy day of obligation</ew><grp>holy day</grp><grp>holy of</grp><grp>holy obligation</grp><grp>of obligation</grp><grp>day of</grp><grp>day obligation</grp><grp>of obligation</grp><subj>RC</subj><hw>holy day of obligation</hw><def><date>1909</date><dt>:a feast on which Roman Catholics are duty-bound to attend mass</dt></def></entry>
