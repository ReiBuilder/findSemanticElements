<entry_list version="1.0">
	<entry id="wine[1]"><ew>wine</ew><subj>AL-1#AL-2#RC-1b#CO-4</subj><hw hindex="1">wine</hw><sound><wav>wine0001.wav</wav><wpr>!wIn</wpr></sound><pr>ˈwīn</pr><fl>noun</fl><lb>often attributive</lb><et>Middle English <it>win,</it> from Old English <it>wīn;</it> akin to Old High German <it>wīn</it> wine; both ultimately from Latin <it>vinum</it> wine, perhaps of non-IE origin; akin to the source of Greek <it>oinos</it> wine</et><def><date>before 12th century</date> <sn>1 a</sn> <dt>:the alcoholic fermented juice of fresh grapes used as a beverage</dt>  <sn>b</sn> <dt>:wine or a substitute used in Christian communion services</dt> <sn>2</sn> <dt>:the alcoholic usually fermented juice of a plant product (as a fruit) used as a beverage <vi>blackberry <it>wine</it></vi></dt> <sn>3</sn> <dt>:something that invigorates or intoxicates</dt> <sn>4</sn> <dt>:a dark red</dt></def></entry>
	<entry id="wine[2]"><ew>wine</ew><subj>AL</subj><hw hindex="2">wine</hw><fl>verb</fl><in><if>wined</if></in><in><if>win*ing</if></in><def><vt>intransitive verb</vt><date>1829</date> <dt>:to drink wine</dt><vt>transitive verb</vt> <dt>:to give wine to <vi><it>wined</it> and dined his friends</vi></dt></def></entry>
	<entry id="wine cellar"><ew>wine cellar</ew><subj>AC#AL</subj><hw>wine cellar</hw><fl>noun</fl><def><date>14th century</date><dt>:a room for storing wines</dt> <sd>also</sd> <dt>:a stock of wines</dt></def></entry>
	<entry id="wine cooler"><ew>wine cooler</ew><subj>HH-1#AL-2</subj><hw>wine cooler</hw><fl>noun</fl><def><date>1815</date> <sn>1</sn> <dt>:a vessel or container in which wine is cooled</dt> <sn>2</sn> <dt>:a usually carbonated beverage that contains a mixture of wine and fruit juice</dt></def></entry>
	<entry id="wine taster"><ew>wine taster</ew><subj>JB-1#HH-2#AL</subj><hw>wine taster</hw><fl>noun</fl><def><date>1632</date> <sn>1</sn> <dt>:a person who tastes and evaluates wine especially professionally</dt> <sn>2</sn> <dt>:a small shallow vessel used to sample wine</dt></def></entry>
	<entry id="blush wine"><ew>blush wine</ew><subj>AL</subj><hw>blush wine</hw><fl>noun</fl><def><date>1985</date><dt>:any of various pinkish table wines</dt></def></entry>
	<entry id="dessert wine"><ew>dessert wine</ew><subj>AL</subj><hw>dessert wine</hw><fl>noun</fl><def><date>1773</date><dt>:a usually sweet wine typically served with dessert or afterward</dt></def></entry>
	<entry id="fortified wine"><ew>fortified wine</ew><subj>AL</subj><hw>fortified wine</hw><fl>noun</fl><def><date>1874</date><dt>:a wine (as sherry) to which alcohol usually in the form of grape brandy has been added during or after fermentation</dt></def></entry>
	<entry id="ice wine"><ew>ice wine</ew><subj>AL</subj><hw>ice wine</hw><fl>noun</fl><def><date>1967</date><dt>:<sx>eiswein</sx></dt></def></entry>
	<entry id="jug wine"><ew>jug wine</ew><subj>AL</subj><hw>jug wine</hw><fl>noun</fl><def><date>1971</date><dt>:table wine sold in large bottles</dt></def></entry>
