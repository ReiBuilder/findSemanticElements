<?xml version="1.0" encoding="utf-8" ?>
<entry_list version="1.0">
	<entry id="fever[1]"><ew>fever</ew><subj>MD-1</subj><hw hindex="1">fe*ver</hw><sound><wav>fever001.wav</wav><wpr>!fE-vur</wpr></sound><pr>ˈfē-vər</pr><fl>noun</fl><et>Middle English, from Old English <it>fēfer,</it> from Latin <it>febris</it></et><def><date>before 12th century</date> <sn>1 a</sn> <dt>:a rise of body temperature above the normal</dt>  <sn>b</sn> <dt>:any of various diseases of which fever is a prominent symptom</dt> <sn>2 a</sn> <dt>:a state of heightened or intense emotion or activity</dt>  <sn>b</sn> <dt>:a contagious usually transient enthusiasm :<sx>craze</sx></dt></def></entry>
	<entry id="fever[2]"><ew>fever</ew><subj>MD-vi</subj><hw hindex="2">fever</hw><fl>verb</fl><in><if>fe*vered</if></in><in><if>fe*ver*ing</if><sound><wav>fever002.wav</wav><wpr>!fE-v(u-)riN</wpr></sound> <pr>ˈfē-vriŋ, ˈfe-vər-iŋ</pr></in><def><vt>transitive verb</vt><date>1606</date> <dt>:to throw into a fever :<sx>agitate</sx></dt><vt>intransitive verb</vt> <dt>:to contract or be in a fever :become <fw>feverish</fw></dt></def></entry>
	<entry id="fever blister"><ew>fever blister</ew><subj>MD#MB</subj><hw>fever blister</hw><fl>noun</fl><def><date>1845</date><dt>:<sx>cold sore</sx></dt></def></entry>
	<entry id="fever pitch"><ew>fever pitch</ew><hw>fever pitch</hw><fl>noun</fl><def><date>1846</date><dt>:a state of intense excitement and agitation</dt></def></entry>
	<entry id="fever tree"><ew>fever tree</ew><subj>BS#MD</subj><hw>fever tree</hw><fl>noun</fl><def><date>1868</date><dt>:any of several shrubs or trees that are thought to indicate regions free from fever or that yield remedies for fever</dt> <sd>especially</sd> <dt>:an African acacia (<it>Acacia xanthlophloea</it>)</dt></def></entry>
	<entry id="African swine fever"><ew>African swine fever</ew><grp>African swine</grp><grp>African fever</grp><grp>swine fever</grp><subj>AG#MD</subj><hw>African swine fever</hw><fl>noun</fl><def><date>1948</date><dt>:<sx>swine fever <sxn>2</sxn></sx></dt></def></entry>
	<entry id="blackwater fever"><ew>blackwater fever</ew><subj>MD</subj><hw>blackwater fever</hw><fl>noun</fl><def><date>1884</date><dt>:a febrile complication of repeated malarial attacks that is characterized especially by extensive kidney damage and urine discolored by heme from blood</dt></def></entry>
	<entry id="breakbone fever"><ew>breakbone fever</ew><subj>MD#EY#MB</subj><hw>break*bone fever</hw><sound><wav>breakb01.wav</wav><wpr>!brAk-!bOn-!fE-vur</wpr></sound><pr>ˈbrāk-ˈbōn-</pr><fl>noun</fl><def><date>1855</date><dt>:<sx>dengue</sx></dt></def></entry>
	<entry id="buck fever"><ew>buck fever</ew><subj>HF#PS</subj><hw>buck fever</hw><fl>noun</fl><def><date>1841</date><dt>:nervous excitement of an inexperienced hunter at the sight of game</dt></def></entry>
	<entry id="cabin fever"><ew>cabin fever</ew><subj>PS</subj><hw>cabin fever</hw><fl>noun</fl><def><date>1918</date><dt>:extreme irritability and restlessness from living in isolation or a confined indoor area for a prolonged time</dt></def></entry>
