<entry_list version="1.0">
	<entry id="taxi[1]"><ew>taxi</ew><subj>TN</subj><hw hindex="1">taxi</hw><sound><wav>taxi0001.wav</wav><wpr>!tak-sE</wpr></sound><pr>ˈtak-sē</pr><fl>noun</fl><in><il>plural</il> <if>tax*is</if><sound><wav>taxi0002.wav</wav><wpr>!tak-sEz</wpr></sound> <pr>-sēz</pr> <il>also</il> <if>tax*ies</if></in><def><date>circa 1907</date><dt>:<sx>taxicab</sx></dt> <sd>also</sd> <dt>:a similarly operated boat or aircraft</dt></def></entry>
	<entry id="taxi[2]"><ew>taxi</ew><subj>AE-vi1#AE-vt2</subj><hw hindex="2">taxi</hw><fl>verb</fl><in><if>tax*ied</if></in><in><if>taxi*ing</if></in><in><if>tax*is</if> <il>or</il> <if>tax*ies</if></in><def><vt>intransitive verb</vt><date>1911</date> <sn>1 a</sn> <ssl>of an aircraft</ssl> <dt>:to go at low speed along the surface of the ground or water</dt>  <sn>b</sn> <dt>:to operate an aircraft on the ground under its own power</dt> <sn>2</sn> <dt>:to ride in a <fw>taxicab</fw> <vi><it>taxi</it><it>ed</it> to the train station</vi></dt><vt>transitive verb</vt> <sn>1</sn> <dt>:to transport by or as if by taxi</dt> <sn>2</sn> <dt>:to cause (an aircraft) to taxi</dt></def></entry>
	<entry id="taxi dancer"><ew>taxi dancer</ew><subj>JB</subj><hw>taxi dancer</hw><fl>noun</fl><def><date>circa 1927</date><dt>:a woman employed by a dance hall, café, or cabaret to dance with patrons who pay an amount for each dance</dt></def></entry>
	<entry id="taxi stand"><ew>taxi stand</ew><subj>TN</subj><hw>taxi stand</hw><fl>noun</fl><def><date>1922</date><dt>:a place where taxis may park while awaiting hire</dt></def></entry>
	<entry id="air taxi"><ew>air taxi</ew><subj>AE#TN</subj><hw>air taxi</hw><fl>noun</fl><def><date>1920</date><dt>:a small commercial airplane used for short flights between localities not served by scheduled airlines</dt></def></entry>
	<entry id="water taxi"><ew>water taxi</ew><subj>NA#TN</subj><hw>water taxi</hw><fl>noun</fl><def><date>1928</date><dt>:a boat functioning (as within a harbor) as a taxi</dt></def></entry>
